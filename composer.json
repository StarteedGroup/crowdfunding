{
    "name": "starteed/crowdfunding",
    "description": "A PHP wrapper for Starteed Crowdfunding's API",
    "keywords": [
        "starteed", "crowdfunding", "api", "wrapper", "client"
    ],
    "homepage": "https://github.com/StarteedGroup/crowdfunding",
    "license": "MIT",
    "authors": [
        {
            "name": "Dario Tranchitella",
            "email": "dario.tranchitella@starteed.com",
            "homepage": "https://starteed.com",
            "role": "Senior Developer"
        }
    ],
    "require": {
        "php" : ">=5.5.9",
        "php-http/discovery": "^1.1",
        "php-http/httplug": "^1.0",
        "php-http/message": "^1.0",
        "symfony/event-dispatcher": "^3.2"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.0"
    },
    "autoload": {
        "psr-4": {
            "Starteed\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Starteed\\Test\\": "tests"
        }
    }
}
